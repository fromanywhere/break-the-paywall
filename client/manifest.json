{
  "name": "break-the-paywall",
  "description": "Share non-free html using your own storage",
  "version": "1",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_name": "Share"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "http://*/*",
    "https://*/*"
  ],
  "manifest_version": 2
}
